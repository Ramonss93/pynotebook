              <ColorMapEntry color="#B4FFBE" quantity="111" label="-25"/>
              <ColorMapEntry color="#64E100" quantity="95" label=""/>
              <ColorMapEntry color="#C8E100" quantity="79" label="-75"/>
              <ColorMapEntry color="#FFE100" quantity="63"  label=""/>
