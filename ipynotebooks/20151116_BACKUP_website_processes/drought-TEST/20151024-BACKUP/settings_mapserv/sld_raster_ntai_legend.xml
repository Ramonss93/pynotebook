<?xml version="1.0" encoding="ISO-8859-1"?>
<StyledLayerDescriptor version="1.0.0" 
    xsi:schemaLocation="http://www.opengis.net/sld StyledLayerDescriptor.xsd" 
    xmlns="http://www.opengis.net/sld" 
    xmlns:ogc="http://www.opengis.net/ogc" 
    xmlns:xlink="http://www.w3.org/1999/xlink" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <NamedLayer>
    <Name>map</Name>
    <UserStyle>
      <Title>SLD Cook Book: Many color gradient</Title>
      <FeatureTypeStyle>
        <Rule>
          <RasterSymbolizer>
            <Opacity>1.0</Opacity>
            <ColorMap type="classes">
              <!--<ColorMapEntry color="#FFFFFF" quantity="0"   label="unknown"  opacity="0" />-->
              <!--<ColorMapEntry color="" quantity="255" label=""/>-->
              <ColorMapEntry color="#7E0000" quantity="229.5" label="0.8"/>
              <ColorMapEntry color="#FF0000" quantity="204" label="0.6"/>
              <ColorMapEntry color="#FF7F00" quantity="178.5" label="0.4"/>
              <ColorMapEntry color="#FFCC00" quantity="153" label="0.2"/>
              <ColorMapEntry color="#FEFE4C" quantity="127.5" label="0"/>
              <ColorMapEntry color="#D9FED9" quantity="102"  label="-0.2"/>
              <ColorMapEntry color="#99EDFE" quantity="76.5"  label="-0.4"/>
              <ColorMapEntry color="#78CBFE" quantity="51"  label="-0.6" />
              <ColorMapEntry color="#3F94FF" quantity="25.5"   label="-0.8" />
              <ColorMapEntry color="#7F00FF" quantity="1"   label=""  opacity="0" />
            </ColorMap>
          </RasterSymbolizer>
        </Rule>
      </FeatureTypeStyle>
    </UserStyle>
  </NamedLayer>
</StyledLayerDescriptor>

