<?xml version="1.0" encoding="ISO-8859-1"?>
<StyledLayerDescriptor version="1.0.0" 
    xsi:schemaLocation="http://www.opengis.net/sld StyledLayerDescriptor.xsd" 
    xmlns="http://www.opengis.net/sld" 
    xmlns:ogc="http://www.opengis.net/ogc" 
    xmlns:xlink="http://www.w3.org/1999/xlink" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <NamedLayer>
    <Name>map</Name>
    <UserStyle>
      <Title>SLD Cook Book: Many color gradient</Title>
      <FeatureTypeStyle>
        <Rule>
          <RasterSymbolizer>
            <Opacity>0.7</Opacity>
            <ColorMap type="classes">
              <!--<ColorMapEntry color="#FFFFFF" quantity="0"   label="unknown"  opacity="0" />-->
              <ColorMapEntry color="#781800" quantity="1"   label="1" />
              <ColorMapEntry color="#B34700" quantity="1"  label="3" />
              <ColorMapEntry color="#F09400" quantity="25"  label="25"/>
              <ColorMapEntry color="#FFBE3B" quantity="51"  label="51"/>
              <ColorMapEntry color="#FFD88A" quantity="76" label="76"/>
              <ColorMapEntry color="#FFFFFF" quantity="102" label="102"/>
              <ColorMapEntry color="#B6D676" quantity="127" label="127 aids"/>
              <ColorMapEntry color="#8BBA2D" quantity="152" label="152"/>
              <ColorMapEntry color="#60A100" quantity="178" label="178"/>
              <ColorMapEntry color="#1B8500" quantity="203" label="209"/>
              <ColorMapEntry color="#006915" quantity="229" label="229"/>
              <ColorMapEntry color="#d3d3d3" quantity="255" label="No Data"/>
            </ColorMap>
          </RasterSymbolizer>
        </Rule>
      </FeatureTypeStyle>
    </UserStyle>
  </NamedLayer>
</StyledLayerDescriptor>

